{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["src/types"]
  },
  "include": ["src/**/*.ts", "src/routes/meterRoutests"],
  "exclude": ["node_modules/**/*", "dist/**/*"]
}
